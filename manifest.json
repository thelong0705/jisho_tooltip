{
  "name": "Jisho Extension",
  "version": "1.0",
  "description": "Translate word from Japanese to English",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["jquery.js", "content.js"]
    }
  ],
  "browser_action": {
    "default_icon": "icon.png"
  }
}